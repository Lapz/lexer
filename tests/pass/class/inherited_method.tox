class Foo {
  fn inFoo() {
    print("in foo");
    return;
  }
}

class Bar < Foo {
  fn inBar() {
    print("in bar");
    return;
  }
}

class Baz < Bar {
  fn init() -> Baz {
    return Baz {};
  }
  fn inBaz() {
    print("in baz");
    return;
  }
}

fn main() {
  let baz = Baz.init();
  baz.inFoo(); // expect:in foo
  baz.inBar(); // expect:in bar
  baz.inBaz(); // expect:in baz
}
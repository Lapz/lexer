class Toggle {
    state:bool;

    fun value() -> bool {
      return this.state;
    }

    fun activate() -> Toggle {
      this.state = !this.state;
      return this;
    }
}



fun main() -> int{
    var toggle  = Toggle{state:true};

    print toggle.activate().value();

    print toggle.activate().value();

    return 0;
}
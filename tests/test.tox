fun fib(n:int) -> int {
  if n < 2 {
    return n;
  } else {
    return fib(n - 2) + fib(n - 1);
  }
}

class Toggle {
    state:bool;

    fun value() -> bool {
      return this.state;
    }

    fun activate() -> Toggle {
      this.state = !this.state;
      return this;
    }
}



